<h2>ユーザ情報編集</h2>
<%= form_with model: @user, url: publics_user_path, local: true do |f| %>
<table>
  <tr>
    <th>お名前</th><td><%= f.text_field :name %></td>
  </tr>
  <tr>
    <th>メールアドレス</th><td><%= f.email_field :email %></td>
  </tr>
  <tr>
    <th>ユーザアイコン画像</th><td><%#= f.attachment_image_tag :profile_image_id %></td>
  </tr>
</table>
<div>
  <%= f.submit "編集内容保存" %>
</div>
<% end %>